using System;

namespace readWords
{
    class Program
    {
        static void Main(string[] args)
        {
            string username = Console.ReadLine();
            string password = Console.ReadLine();

            while (true)
            {
                string inputPasword = Console.ReadLine();

                if (inputPasword == password)
                {
                    Console.WriteLine($"Welcome {username}!");
                    break;
                }

            }
        }
    }
}
